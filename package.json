{
	"name": "nahuelio.github.io",
	"version": "1.0.0",
	"description": "Nahuel IO Official Website",
	"main": "svelte.config.js",
	"type": "module",
	"scripts": {
		"build": "vite build && bundlesize",
		"build:storybook": "storybook build",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"dev": "vite dev",
		"format": "prettier --plugin-search-dir . --write .",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"preview": "vite preview",
		"storybook": "storybook dev -p 6006"
	},
	"repository": {
		"type": "git",
		"url": "git+ssh://git@github.com/nahuelio/nahuelio.github.io.git"
	},
	"keywords": [
		"nahuelio",
		"portfolio",
		"patricio",
		"ferreira",
		"software",
		"engineer",
		"designer",
		"ux",
		"ui"
	],
	"author": "kuakman <3dimentionar@gmail.com>",
	"license": "ISC",
	"bugs": {
		"url": "https://github.com/nahuelio/nahuelio.github.io/issues"
	},
	"homepage": "https://github.com/nahuelio/nahuelio.github.io#readme",
	"devDependencies": {
		"@storybook/addon-a11y": "7.0.22",
		"@storybook/addon-essentials": "7.0.22",
		"@storybook/addon-interactions": "7.0.22",
		"@storybook/addon-links": "7.0.22",
		"@storybook/addon-styling": "1.3.0",
		"@storybook/addon-svelte-csf": "3.0.3",
		"@storybook/blocks": "7.0.22",
		"@storybook/svelte": "7.0.22",
		"@storybook/sveltekit": "7.0.22",
		"@sveltejs/adapter-static": "2.0.2",
		"@sveltejs/kit": "1.20.4",
		"@sveltejs/svelte-scroller": "2.0.7",
		"@sveltejs/vite-plugin-svelte": "2.4.1",
		"@typescript-eslint/eslint-plugin": "5.59.11",
		"@typescript-eslint/parser": "5.59.11",
		"autoprefixer": "10.4.14",
		"bundlesize": "0.18.1",
		"eslint": "8.43.0",
		"eslint-config-prettier": "8.8.0",
		"eslint-plugin-svelte": "2.31.0",
		"postcss": "8.4.24",
		"postcss-inline-svg": "6.0.0",
		"postcss-nested": "6.0.1",
		"postcss-sorting": "8.0.2",
		"postcss-svgo": "6.0.0",
		"prettier": "2.8.8",
		"prettier-plugin-svelte": "2.10.1",
		"storybook": "7.0.22",
		"svelte": "3.59.1",
		"svelte-check": "3.4.3",
		"svelte-icons": "2.1.0",
		"tailwindcss": "3.3.2",
		"tslib": "2.5.3",
		"typescript": "5.1.3",
		"vite": "4.3.9",
		"vite-tsconfig-paths": "4.2.0"
	},
	"bundlesize": [
		{
			"path": "./build/_app/immutable/nodes/*.js",
			"maxSize": "30 kB"
		},
		{
			"path": "./build/_app/immutable/entry/*.js",
			"maxSize": "150 kB"
		}
	]
}
